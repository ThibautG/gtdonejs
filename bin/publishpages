#!/bin/bash

. $(dirname $0)/.env

s3_bucket="gtdonejs"
files=$(ls ${project_dir}/s3/*.html)

for file in ${files}; do
    filename=$(basename ${file})
    echo "Publishing ${filename}..."
    s3cmd put --acl-public --guess-mime-type ${file} s3://${s3_bucket}/${filename}
    ret=$?
    if [[ $ret != 0 ]]; then
        exit $ret
    fi
done